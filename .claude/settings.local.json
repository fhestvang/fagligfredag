{
  "permissions": {
    "allow": [
      "Bash(git:*)",
      "Bash(gh:*)",
      "Bash(dbt:*)",
      "Bash(python:*)",
      "Bash(pip:*)",
      "Bash(npm:*)",
      "Bash(npx:*)",
      "Bash(powershell:*)",
      "Bash(powershell.exe:*)",
      "Bash(dir:*)",
      "Bash(tree:*)",
      "Bash(find:*)",
      "Bash(timeout:*)",
      "Bash(cd:*)",
      "Bash(gh api:*)",
      "WebFetch",
      "WebSearch"
    ],
    "deny": [
      "Read(**/.env)",
      "Read(**/.env.*)",
      "Read(**/secrets.*)",
      "Read(**/credentials.*)",
      "Read(**/*_secret*)",
      "Read(**/profiles.yml)",
      "Edit(**/.env)",
      "Edit(**/.env.*)",
      "Edit(**/secrets.*)",
      "Edit(**/credentials.*)",
      "Write(**/.env)",
      "Write(**/.env.*)",
      "Write(**/secrets.*)",
      "Write(**/credentials.*)"
    ]
  }
}
